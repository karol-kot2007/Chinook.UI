<Window x:Class="Chinook.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Chinook.UI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">

  <StackPanel Orientation="Vertical">
    <StackPanel Orientation="Horizontal">

      <Button Content="View Album" Margin="320,0,0,0" Click="Button_View_Click"/>
      <Button Content="Edit Album" Margin="5,0,0,0" Click="Button_Edit_Click" />
    </StackPanel>
  </StackPanel>
</Window>
